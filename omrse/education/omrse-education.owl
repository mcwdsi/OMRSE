                                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/OMRSE_00002029"/>
                            </owl:Restriction>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000005"/>
        <obo:IAO_0000111 xml:lang="en">educational objective specification</obo:IAO_0000111>
        <obo:IAO_0000115 xml:lang="en">An objective specification that describes the type of capability or capabilities to be imparted by its realization, which is an educating (education process).</obo:IAO_0000115>
        <obo:IAO_0000117 xml:lang="en">William R. Hogan</obo:IAO_0000117>
        <obo:IAO_0000600 xml:lang="en">An objective specification that describes the new knowledge &amp; skill(s) to be obtained via an education process.</obo:IAO_0000600>
        <rdfs:label xml:lang="en">educational objective</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OMRSE_00002031 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OMRSE_00002031">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
